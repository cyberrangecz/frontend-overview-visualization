{
  "name": "@muni-kypo-crp/overview-visualization",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint && npm run prettier-check",
    "e2e": "ng e2e",
    "prettier-check": "prettier --check \"projects/**/*.ts\"",
    "build-example-app": "ng build kypo2-trainings-visualization-overview --prod",
    "build-library": "ng build --prod kypo2-trainings-visualization-overview-lib && cp ./projects/kypo2-trainings-visualization-overview-lib/src/lib/styles.css ./dist/kypo2-trainings-visualization-overview-lib",
    "create-package": "cd dist/kypo2-trainings-visualization-overview-lib && npm pack",
    "build-and-pack": "npm install && npm run build-library && npm run create-package",
    "ci-update-version": "cd ./projects/kypo2-trainings-visualization-overview-lib/ && npm version $VERSION",
    "ci-build-and-pack": "npm ci --cache .npm --prefer-offline && npm run build-library && npm run create-package",
    "ci-publish-package": "cd dist/kypo2-trainings-visualization-overview-lib && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier-check"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.8",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.8",
    "@angular/compiler": "^11.0.8",
    "@angular/core": "^11.0.8",
    "@angular/forms": "^11.0.8",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "^11.0.8",
    "@angular/platform-browser-dynamic": "^11.0.8",
    "@angular/router": "^11.0.8",
    "@sentinel/auth": "~11.0.0",
    "angular-oauth2-oidc": "~10.0.3",
    "angular-oauth2-oidc-jwks": "9.0.0",
    "core-js": "^2.6.9",
    "@muni-kypo-crp/d3-service": "~11.0.0",
    "d3": "~5.16.0",
    "rxjs": "^6.5.4",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1100.6",
    "@angular-devkit/build-angular": "~0.1100.6",
    "@angular/cli": "~11.0.6",
    "@angular/compiler-cli": "^11.0.8",
    "@angular/language-service": "^11.0.8",
    "@angular-eslint/builder": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin": "^0.8.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "^0.8.0-beta.0",
    "@angular-eslint/schematics": "^0.8.0-beta.0",
    "@angular-eslint/template-parser": "^0.8.0-beta.0",
    "@typescript-eslint/parser": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "eslint": "^7.13.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.5",
    "@types/d3": "~5.16.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~5.0.1",
    "typescript": "~4.0.5"
  }
}
